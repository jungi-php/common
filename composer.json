{
    "name": "jungi/common",
    "description": "A minimal library that defines primitive building blocks of PHP code.",
    "keywords": ["equatable"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Kugla",
            "email": "piku235@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Jungi\\Common\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jungi\\Common\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
