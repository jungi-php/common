{
    "name": "jungi/core",
    "description": "A core library that adds extra basic features to PHP.",
    "keywords": ["result", "option", "rust"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Kugla",
            "email": "piku235@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "files": ["src/functions.php"],
        "psr-4": {
            "Jungi\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jungi\\Core\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
